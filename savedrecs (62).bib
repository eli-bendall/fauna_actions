Title = {{Growth and whole body composition of lake trout (Salvelinus namaycush),
   brook trout (Salvelinus fontinalis) and their hybrid, F-1 splake
   (Salvelinus namaycush x Salvelinus fontinalis), from first-feeding to 16
   weeks post first-feeding}},
Journal = {{AQUACULTURE}},
Year = {{2005}},
Volume = {{249}},
